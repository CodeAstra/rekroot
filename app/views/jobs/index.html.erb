<div class="container-fluid" id ="all_jobs_page">
  <div class="container" id="jobs_container">
    <div class="row" id ="jobs_div">
      <div class="col-md-3 job_card_div">    
        <div class="job_card">
          <%= render partial: 'jobs/new', locals: {company: @company, job: @new_job} %>  
        </div>        
      </div>
        <% @jobs.each do |job| %>
          <%= render partial: 'jobs/job', locals: {company: @company, job: job} %>
        <% end %>
      </div>
  </div>
</div>  

