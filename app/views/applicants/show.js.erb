$('#applicant_details_<%= @applicant.id %>').remove();
$('#applicant_<%= @applicant.id %>').append('<%= j render(partial: "applicants/lightbox", locals: {applicant: @applicant}) %>');
$('#applicant_details_<%= @applicant.id %>').modal('show');
$("#applicant_details_<%= @applicant.id %> .status #status_<%= @applicant.status %>").addClass("highlight");


